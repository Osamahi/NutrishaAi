{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SupabaseUrl": "https://your-project.supabase.co",
    "SupabaseKey": "your-anon-key",
    "SupabaseServiceKey": "your-service-key"
  },
  "AzureStorage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=youraccount;AccountKey=yourkey;EndpointSuffix=core.windows.net",
    "ContainerNames": {
      "UserUploads": "user-uploads",
      "DietPlans": "diet-plans",
      "ProfilePictures": "profile-pictures"
    },
    "MaxFileSizes": {
      "Image": 10485760,
      "Voice": 5242880,
      "Document": 20971520,
      "Video": 52428800
    }
  },
  "Gemini": {
    "ApiKey": "your-gemini-api-key",
    "Model": "gemini-1.5-pro",
    "MaxTokens": 8192,
    "Temperature": 0.7,
    "AudioConfig": {
      "SupportedFormats": ["webm", "mp3", "wav", "m4a"],
      "MaxDuration": 300
    }
  },
  "Qdrant": {
    "Host": "localhost",
    "Port": 6333,
    "ApiKey": "",
    "CollectionName": "nutrisha_conversations",
    "VectorSize": 1536,
    "UseHttps": false
  },
  "Jwt": {
    "Key": "your-super-secret-jwt-key-min-32-characters-long",
    "Issuer": "NutrishaAI",
    "Audience": "NutrishaAI",
    "ExpiryInMinutes": 60
  },
  "ApiKeySettings": {
    "HeaderName": "X-API-Key",
    "UserIdHeaderName": "X-User-Id",
    "RateLimitPerHour": 1000
  },
  "SupabaseRealtime": {
    "ChatChannel": "chat",
    "NotificationChannel": "notifications",
    "HealthDataChannel": "health_updates"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000", "http://localhost:5173"]
  }
}